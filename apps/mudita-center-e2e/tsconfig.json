{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "types": [
      "node",
      "@wdio/globals/types",
      "@wdio/mocha-framework",
      "expect-webdriverio"
    ],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "es5",
    "outDir": "./dist",
    "typeRoots": ["./node_modules/@types", "./typings"]
  },
  "include": ["src/**/*", "./wdio.conf.ts"],
  "exclude": ["node_modules"]
}
