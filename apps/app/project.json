{
  "name": "app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/app/src",
  "projectType": "application",
  "tags": ["process:main"],
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "electron-vite dev",
        "cwd": "apps/app"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "electron-vite preview",
        "cwd": "apps/app"
      }
    },
    "e2e": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "nx start app-e2e"
      }
    },
    "build:mac": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "electron-vite build && electron-builder --mac --publish never",
        "cwd": "apps/app"
      }
    },
    "build:linux": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "electron-vite build && electron-builder --linux --publish never",
        "cwd": "apps/app"
      }
    },
    "build:win": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "electron-vite build && electron-builder --win --publish never",
        "cwd": "apps/app"
      }
    }
  }
}
