{
  "extends": ["../../../.eslintrc.js"],
  "ignorePatterns": ["!**/*", "styled.d.ts"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {
        "@typescript-eslint/no-unsafe-return": "off",
        "value-no-vendor-prefix": "off"
      }
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
